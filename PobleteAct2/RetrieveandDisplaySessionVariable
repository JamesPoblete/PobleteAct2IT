<?php
session_start();
if (isset($_SESSION["userId"]) && isset($_SESSION["userName"]) && isset($_SESSION["Age"])) {
    $userId = $_SESSION["userId"];
    $userName = $_SESSION["userName"];
    $age = $_SESSION["Age"];
    
    echo "userId: $userId<br>";
    echo "userName: $userName<br>";
    echo "Age: $age<br>";
} else {
    echo "Session variables not set.";
}
?>
